  "binary": {
    "module_name": "node_printer",
    "module_path": "./lib/",
    "host": "https://github.com/Marlonzao/marlon-node-printer/releases/download/",
    "remote_path": "v{version}"
  },
  "licenses": [
    {
      "type": "BSD"
    }
  ],
  "main": "./lib/printer",
  "dependencies": {
    "nan": "^2.23.0",
