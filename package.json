{
  "name": "@eusoumarlon/node-printer",
  "description": "Node.js printer bindings",
  "version": "0.6.5",
  "homepage": "https://github.com/Marlonzao/marlon-node-printer",
  "author": {
    "name": "Ion Lupascu",
    "url": "https://multipedidos.com.br/",
    "email": "marlon@multipedidos.com.br"
  },
  "contributors": [
    "Thiago Lugli <thiagoelg@gmail.com>",
    "Eko Eryanto <ekoeryanto@gmail.com>",
    "Ivan Nikolic <ivannkf@gmail.com>",
    "Marlon Oliveira <marlon@multipedidos.com.br>"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/Marlonzao/marlon-node-printer.git"
  },
  "devDependencies": {
    "node-abi": "^4.14.0",
    "node-gyp": "^10.0.1",
    "nodeunit": "^0.11.3",
    "patch-package": "^8.0.0",
    "prebuild": "^13.0.0"
  },
  "engines": {
    "node": ">= 4.0.0"
  },
  "scripts": {
    "install": "prebuild-install || node-gyp rebuild",
    "apply-patches": "patch-package",
    "prebuild-all": "prebuild --all --force --strip --verbose --openssl_fips=X",
    "prebuild": "prebuild",
    "rebuild": "node-gyp rebuild",
